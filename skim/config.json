{
  "agent_servers": {
    "Claude Code": {
      "command": "claude-code-acp",
      "env": {
        "CLAUDE_CODE_OAUTH_TOKEN": "${CLAUDE_CODE_OAUTH_TOKEN}",
        "MAX_THINKING_TOKENS": "32000"
      },
      "skim": {
        "default": true,
        "mode": "bypassPermissions",
        "model": "opus"
      }
    },
    "Codex": {
      "command": "codex-acp"
    },
    "OpenCode": {
      "protocol": "opencode",
      "command": "opencode"
    }
  }
}
